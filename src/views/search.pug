extends layouts/main.pug
include mixins/videoBlock

block content
    .search-header
        h3  Searching by #{searchingBy}
    .search_videos
        if videos.length === 0
            h5 No Videos Found
        each item   in videos
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl,
                id: item.id
            })
